<% include ./partials/header %>

<h1>Selection of my travel pics</h1>


<% posts.forEach(function(post) { %>
    <div>
        <h2><%= post.title %></h2>
        <img src='<%= post.image%>' >
        <span><%= post.created %></span>
        <p><%= post.body %></p>
    </div>
<% }); %>


